import io;

class myClass {
    int a;

    public int get() {
        return a;
    }

    public int put(int n){
        a = n;
        return 0;
    }

    public int m1(){
        a = 2;
        io.println(a);
        myClass c1;
        io.println(c1.get());
        c1.put(2);
        io.println(c1.get());
    }

    public static void main(String[] args){
        myClass A;
        A.m1();
    }
}