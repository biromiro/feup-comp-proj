import ioPlus;
class Recursion {

    public int fib(int n) {
        int result;
        if (n < 2) {
            result = n;
        } else {
            result = this.fib(n - 1) + this.fib(n - 2);
        }
        return result;
    }

	public static void main(String[] args) {
	    Recursion r;

	    r = new Recursion();

	    ioPlus.printResult(r.fib(10));
	}
}