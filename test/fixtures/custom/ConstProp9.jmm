import ioPlus;
class ConstProp {
	public static void main(String[] args) {
	    int i;
	    int x;
	    i = 1;
	    x = 50;

	    while (i < 10) {
	        ioPlus.printResult(x);
	        if (3 < i && i < 8) {
	            x = 500;
	            ioPlus.printResult(x);
	        } else {}
	        i = i * 2;
	    }
	}
}
