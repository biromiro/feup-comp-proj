import ioPlus;
class Recursion {

    public int fact(int n) {
        int result;
        if (n < 2)
            result = 1;
        else
            result = n * this.fact(n - 1);

        return result;
    }

	public static void main(String[] args) {
	    Recursion r;

	    r = new Recursion();

	    ioPlus.printResult(r.fact(5));
	}
}